{
    "apiVersion": "v1",
    "items": [
        {
            "apiVersion": "extensions/v1beta1",
            "kind": "Deployment",
            "metadata": {
                "name": "seldon-apiserver"
            },
            "spec": {
                "replicas": 1,
                "template": {
                    "metadata": {
                        "annotations": {
                            "prometheus.io/path": "/prometheus",
                            "prometheus.io/port": "8080",
                            "prometheus.io/scrape": "true"
                        },
                        "labels": {
                            "app": "seldon-apiserver-container-app",
                            "version": "1"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [
                                    {
                                        "name": "SELDON_ENGINE_ZK_SERVERS",
                                        "value": "zookeeper-1:2181,zookeeper-2:2181,zookeeper-3:2181"
                                    },
                                    {
                                        "name": "SELDON_ENGINE_KAFKA_SERVER",
                                        "value": "kafka:9092"
                                    },
                                    {
                                        "name": "SELDON_CLUSTER_MANAGER_REDIS_HOST",
                                        "value": "redis"
                                    }
                                ],
                                "image": "seldonio/apife:%APIFE_IMAGE_VERSION%",
                                "name": "seldon-apiserver-container",
                                "imagePullPolicy": "IfNotPresent",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "protocol": "TCP"
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "seldon-apiserver",
                "labels": {
                    "app": "seldon-apiserver-container-app"
                }
            },
            "spec": {
                "ports": [
                    {
                        "name": "http",
                        "nodePort": 30032,
                        "port": 8080,
                        "protocol": "TCP",
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "app": "seldon-apiserver-container-app"
                },
                "sessionAffinity": "None",
                "type": "%SELDON_APIFE_SERVICE_TYPE%"
            },
            "status": {
                "loadBalancer": {}
            }
        }
    ],
    "kind": "List",
    "metadata": {}
}
