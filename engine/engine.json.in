{
    "apiVersion": "v1",
    "items": [
        {
            "apiVersion": "v1",
            "kind": "Pod",
            "metadata": {
                "labels": {
                    "app": "seldon-engine-container-app"
                },
                "name": "seldon-engine"
            },
            "spec": {
                "containers": [
                    {
                        "env": [
                            {
                                "name": "SELDON_ENGINE_ZK_SERVERS",
                                "value": "zookeeper-1:2181,zookeeper-2:2181,zookeeper-3:2181"
                            },
                            {
                                "name": "SELDON_ENGINE_POD_NAMESPACE",
                                "valueFrom": {
                                    "fieldRef": {
                                        "apiVersion": "v1",
                                        "fieldPath": "metadata.namespace"
                                    }
                                }
                            }
                        ],
                        "image": "seldonio/engine:%ENGINE_IMAGE_VERSION%",
                        "name": "seldon-engine-container",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "seldon-engine"
            },
            "spec": {
                "ports": [
                    {
                        "name": "client",
                        "nodePort": 30031,
                        "port": 8080,
                        "protocol": "TCP",
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "app": "seldon-engine-container-app"
                },
                "sessionAffinity": "None",
                "type": "NodePort"
            },
            "status": {
                "loadBalancer": {}
            }
        }
    ],
    "kind": "List",
    "metadata": {}
}
